{
  "name": "techlogs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "db:dev:up": "docker compose -f docker-compose.yml up techlogdb_dev -d",
    "db:dev:down": "docker compose -f docker-compose.yml down techlogdb_dev",
    "dev": "next dev --turbopack",
    "first:dev": "npm run db:dev:up && next dev --turbopack",
    "build": "prisma generate && prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typegen": "sanity schema extract --path=./sanity/extract.json && sanity typegen generate"
  },
  "dependencies": {
    "@auth/core": "^0.40.0",
    "@auth/prisma-adapter": "^2.10.0",
